
@model IEnumerable<TennisCoach.Models.Coaches>
@if (TempData["SuccessMessage"] != null)







{
    <div class="alert alert-success">@TempData["SuccessMessage"]</div>
}

@if (TempData["ErrorMessage"] != null)







{
    <div class="alert alert-danger">@TempData["ErrorMessage"]</div>
}
<h2>Enrolled Coach</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Active</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var Coach in Model)
        {
            <tr>
                <td>@Coach.FirstName</td>
                <td>@Coach.LastName</td>
            
            </tr>
        }
    </tbody>
</table>
<div style="text-align: center; margin-top: 20px;">
   <a asp-controller="Coach" asp-action="Index" class="btn" style="background-color: #2c3e50; border: none; padding: 10px 20px; border-radius: 4px; color: white; font-size: 16px; cursor: pointer;">Back</a>
</div>
